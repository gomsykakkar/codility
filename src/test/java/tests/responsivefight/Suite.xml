		</test>
		<test name="RF - Bus Challenge Grid Win Opera" preserve-order="true">
			<classes><class name="tests.responsivefight.busChallenge"></class></classes>
			<parameter name="Browser" value="grid-win-opera" />
			<parameter name="Groups" value="WEB,ResponsiveFight,Bus" />
			<parameter name="Author" value="GauravK" />
		</test>
	<listeners>
		<listener class-name="com.gaurav.listener.CommonTestListener" />
	</listeners>		
</suite>